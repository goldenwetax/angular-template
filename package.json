{
  "name": "angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4500 --open",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod --build-optimizer --vendor-chunk=true",
    "build:dev": "ng build",
    "build:optimizer": "ng build --prod --build-optimizer",
    "build:server": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --vendor-chunk=true",
    "build:analyzer": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --stats-json --prod --build-optimizer=false --vendor-chunk=true",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "deploy": "aliyun-oss-deploy -p ./dist/webapp -c .aliossrc -d static",
    "node:version": "node -v && npm -v && tsc --version",
    "preview": "http-server dist/webapp -o -p 3011"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@ant-design/icons-angular": "^8.0.0",
    "@mojs/core": "^0.288.2",
    "@ngx-loading-bar/core": "^5.1.1",
    "@ngx-loading-bar/http-client": "^5.1.1",
    "@ngx-loading-bar/router": "^5.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "echarts": "^4.2.1",
    "echarts-liquidfill": "^2.0.5",
    "echarts-wordcloud": "^1.1.3",
    "font-awesome": "^4.7.0",
    "headroom.js": "^0.9.4",
    "ng-zorro-antd": "^11.0.2",
    "ngx-bootstrap": "^6.2.0",
    "ngx-echarts": "^4.2.1",
    "ngx-ueditor": "^2.1.3",
    "normalize.css": "^8.0.1",
    "photoswipe": "^4.1.3",
    "require": "^2.4.20",
    "rxjs": "~6.6.0",
    "screenfull": "^4.2.0",
    "tslib": "^2.0.0",
    "uuid": "^3.4.0",
    "video.js": "^7.5.5",
    "videojs-flash": "^2.2.0",
    "viewerjs": "^1.4.0",
    "xlsx": "^0.15.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@angular/language-service": "~8.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "aliyun-oss-deploy": "^0.1.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^3.9.0"
  }
